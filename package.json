		"koa": "^2.15.3",
		"koa-static": "^5.0.0"
	},
	"scripts": {
		"dev": "wireit",
		"build": "wireit",
		"server:build": "wireit"
	},
	"wireit": {
		"server:build": {
			"command": "tsc --project ./tsconfig.server.json",
			"files": [
				"./tsconfig.server.json",
				"./src/server.ts"
			],
			"output": [
				"./server.js"
			]
		},
		"dev": {
			"command": "vite --host"
		},
		"build": {
			"command": "vite build",
			"files": [
				"vite.config.ts",
				"src"
			]
		}
	}
}
